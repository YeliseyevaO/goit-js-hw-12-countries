<h1 class="card-title">{{name}}</h1>
<div class="countryCard">   
    <div class="card-body">
<h2>Capital:</h2>
<p>{{capital}}</p>
<h2>Population:</h2>
<p>{{population}}</p>
<h2>Languages:</h2>
<ul class="list-group">
{{#each languages}}
<li>{{name}}</li>
{{/each}}
</ul>
</div>
<div class="card-img">
    <img src="{{flag}}" alt="{{name}}">
</div>

</div>